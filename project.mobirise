          "_customHTML": "<section class=\"tabs tabs2 modernm4_tabs2\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" group=\"Tabs\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input inline max=\"10\" min=\"0\" name=\"paddingTop\" step=\"1\" title=\"Top\" type=\"range\" value=\"10\">\n        <input max=\"10\" min=\"0\" name=\"paddingBottom\" step=\"1\" title=\"Bottom\" type=\"range\" value=\"10\">\n        <header>Show/Hide</header>\n        <input checked name=\"showButtons\" title=\"Show Buttons\" type=\"checkbox\">\n        <input name=\"accentColor\" title=\"Acccent Color\" type=\"color\" value=\"#ff6400\">\n        <input name=\"tabsColor\" title=\"Tabs Color\" type=\"color\" value=\"#ffffff\">\n        <select name=\"bootstrapTabs\" title=\"Tabs Count\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <header>Background</header>\n        <fieldset name=\"bg\" parallax type=\"background\">\n            <input parallax title=\"Image\" type=\"image\" value=\"@PROJECT_PATH@/assets/images/hero1.3640cee-1-2000x1266.jpg\" selected>\n            <input title=\"Color\" type=\"color\" value=\"#1e2149\">\n            <input title=\"Video\" type=\"video\" value=\"http://www.youtube.com/watch?v=uNCr7NdOJgw\">\n        </fieldset>\n        <input condition=\"bg.type !== 'color'\" name=\"overlay\" title=\"Overlay\" type=\"checkbox\" checked>\n        <input condition=\"overlay && bg.type !== 'color'\" name=\"overlayColor\" title=\"Overlay Color\" type=\"color\" value=\"#5842ff\">\n        <input condition=\"overlay && bg.type !== 'color'\" inline max=\"1\" min=\"0\" name=\"overlayOpacity\" step=\"0.1\" title=\"Opacity\" type=\"range\" value=\"0.5\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n\n    <div class=\"container\">\n        <div class=\"inner__container\">\n            <h3 class=\"mbr-section-title mbr-medium mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\">Adopting ₿itcoin&nbsp;</h3>\n            <div class=\"image\">\n                <img alt src=\"@PROJECT_PATH@/assets/images/logo.c8e5138-843x843.png\" title>\n            </div>\n            <div class=\"tabs-container\">\n                <ul class=\"nav nav-tabs\" data-app-placeholder=\"Type Title\" role=\"tablist\">\n                    <li class=\"nav-item\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\" mbr-buttons mbr-theme-style=\"display-7\" role=\"presentation\"><a aria-controls=\"tech-tab1-content\" aria-selected=\"true\" class=\"nav-link mbr-fonts-style show active\" data-app-placeholder=\"Type Text\" data-toggle=\"tab\" href=\"#tabs2-o_tab0\" role=\"tab\">Overview.</a></li>\n                    <li class=\"nav-item\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\" mbr-buttons mbr-if=\"bootstrapTabs > 1\" mbr-theme-style=\"display-7\" role=\"presentation\"><a aria-controls=\"tech-tab2-content\" aria-selected=\"true\" class=\"nav-link mbr-fonts-style active\" data-app-placeholder=\"Type Text\" data-toggle=\"tab\" href=\"#tabs2-o_tab1\" role=\"tab\"> Documentation. </a></li>\n                    <li class=\"nav-item\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\" mbr-buttons mbr-if=\"bootstrapTabs > 2\" mbr-theme-style=\"display-7\" role=\"presentation\"><a aria-controls=\"tech-tab3-content\" aria-selected=\"true\" class=\"nav-link mbr-fonts-style active\" data-app-placeholder=\"Type Text\" data-toggle=\"tab\" href=\"#tabs2-o_tab2\" role=\"tab\"> Inter